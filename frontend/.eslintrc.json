{
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "eqeqeq": "off",
      "no-console": "off",
      "vue/multi-word-component-names": "off",
      "vue/valid-template-root": "off"
    },
    "globals": {
      "FB": "readonly"
    }
}